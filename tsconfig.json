{
  "compilerOptions": {
    "jsx": "react",
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "allowJs": true
  },
  "exclude": [
    "./node_modules/**/*"
  ],
  "include": ["src"]
}
